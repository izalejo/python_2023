# print("El resultado de la suma es: ", resultadosuma)
# print("El resultado de la resta es: ", resultadoresta)      

def Sumar(*valores):
    resultado = 0
    for item in valores:
        resultado += item
    return resultado

resultado = Sumar(23,56,3,89,78,455)
print("El resultado de la suma es: ", resultado)
